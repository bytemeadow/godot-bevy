[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3

[libraries]
linux.debug.x86_64 = "res://../../../target/debug/librust_simple_node2d_movement.so"
linux.release.x86_64 = "res://../../../target/release/librust_simple_node2d_movement.so"
macos.debug.x86_64 = "res://../../../target/debug/librust_simple_node2d_movement.dylib"
macos.release.x86_64 = "res://../../../target/release/librust_simple_node2d_movement.dylib"
macos.debug.arm64 = "res://../../../target/debug/librust_simple_node2d_movement.dylib"
macos.release.arm64 = "res://../../../target/release/librust_simple_node2d_movement.dylib"
windows.debug.x86_64 = "res://../../../target/debug/rust_simple_node2d_movement.dll"
windows.release.x86_64 = "res://../../../target/release/rust_simple_node2d_movement.dll"

# Web platforms (requires Godot 4.3+)
# Non-threaded builds (broader browser compatibility)
web.debug.wasm32 = "res://../../../target/wasm32-unknown-emscripten/debug/rust_simple_node2d_movement.wasm"
web.release.wasm32 = "res://../../../target/wasm32-unknown-emscripten/release/rust_simple_node2d_movement.wasm"
# Threaded builds (requires SharedArrayBuffer / cross-origin isolation)
web.debug.threads.wasm32 = "res://../../../target/wasm32-unknown-emscripten/debug/rust_simple_node2d_movement.threads.wasm"
web.release.threads.wasm32 = "res://../../../target/wasm32-unknown-emscripten/release/rust_simple_node2d_movement.threads.wasm"
